 #ifndef __GATT_SERVICE_DB__
 #define __GATT_SERVICE_DB__
/* For service details, refer http://developer.bluetooth.org/gatt/services/ 
 * Pages/ServiceViewer.aspx?u=org.bluetooth.service.generic_access.xml 
 */
 
/*!< \brief Generic Attribute Service */
#define UUID_GATT                                       0x1801  
/* All the names will be used in corresponding generated handle names */

primary_service {
    uuid : UUID_GATT,
    name : "GATT_SERVICE"
    /* No characteristic supported for this service */
},  /* This comma is required to be added only if there are more .db files 
     * included after this file in app_gatt_db.db 
     */
#endif /* __GATT_SERVICE_DB__ */